# OpenAI API Key for AI analysis
OPENAI_API_KEY=your_openai_api_key_here

# GitHub Personal Access Token for GitHub CLI authentication
# Create token at: https://github.com/settings/tokens
# Required scopes: repo, read:org, read:user, user:email
GITHUB_TOKEN=your_github_token_here

# SMTP Configuration for Email Sending (using Brevo/Sendinblue)
SMTP_SERVER=smtp-relay.brevo.com
SMTP_PORT=587
SMTP_USERNAME=your_brevo_login_email
SMTP_PASSWORD=your_brevo_smtp_key
FROM_EMAIL=tinker@sprited.app
FROM_NAME=Tinker AI Agent


